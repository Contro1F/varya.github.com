.header
    -webkit-box-ordinal-group: 1;
    -moz-box-ordinal-group: 1;
    -ms-flex-order: 1;
    -webkit-order: 1;
    order: 1;
    width: 1200px;
    margin: 2em auto 3em;
    text-align: left;
    padding: 0;
    display: table;

    &__left
        padding: 0;
        margin: 0;
        width: auto;
        display: table-cell;
        text-align: right;
        width: 778px;

    &__center
        padding: 0;
        margin: 0;
        display: table-cell;
        width: 200px;

    &__right
        width: 325px;
        padding-left: 20px;
        display: table-cell;

    &__item
        border-radius: 50%;
        margin-bottom: 30px;
        margin-right: 3px;
        display: inline-block;
